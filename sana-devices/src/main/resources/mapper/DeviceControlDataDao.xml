<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sana.devices.dao.DeviceControlDataDao">

    <select id="getDeviceControlDataList"
            resultType="com.sana.devices.entity.vo.result.DeviceControlDataResult">
        SELECT
        *
        FROM
        device_control_data
        where device_item_id = #{query.deviceItemId}
        AND deleted = 0

    </select>

</mapper>